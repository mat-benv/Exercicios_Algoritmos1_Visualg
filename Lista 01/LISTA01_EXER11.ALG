algoritmo "lista1_exer11"
var
   temp_f, temp_c: real

inicio
      escreva("Digite a temperatura em graus Farenheit: ")
      leia(temp_f)
      temp_c <- (temp_f - 32)*(5/9)
      escreval("Temperatura em graus Farenheit =", temp_f)
      escreva("Temperatura em graus Celsius =", temp_c)

fimalgoritmo
